{
    "version": 2,
    "framework": null,
    "builds": [
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "public"
        }
    }
    ],
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [{
        "src": "/(.*)",
        "dest": "/api/index.php"
    }]
}